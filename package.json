{
    "name": "parana-network",
    "version": "0.1.0",
    "private": true,
    "bin": "server.js",
    "dependencies": {
        "@deck.gl/geo-layers": "^8.9.27",
        "@deck.gl/react": "^8.9.27",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^13.5.0",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "deck.gl": "^8.9.27",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "framer-motion": "^12.23.26",
        "jsonwebtoken": "^9.0.0",
        "lucide-react": "^0.474.0",
        "mapbox-gl": "^2.15.0",
        "mysql2": "^3.11.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-map-gl": "^7.0.15",
        "react-scripts": "^5.0.1",
        "source-map-loader": "0.2.4",
        "sql.js": "^1.8.0",
        "uuid": "^9.0.0",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "build:login": "npm --prefix Login run build",
        "prestart": "node scripts/copy-brand-assets.js && npm --prefix Login run build && node scripts/copy-login-build.js",
        "gateway": "node tools/gateway.js",
        "start": "react-scripts start",
        "prebuild": "node scripts/copy-brand-assets.js",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "server": "node server.js",
        "seed-admin": "node auth/seed_admin.js",
        "dev": "concurrently -k -n API,WEB -c magenta,cyan \"npm run server\" \"npm start\"",
        "dev:gateway": "concurrently -k -n API,WEB,GW -c magenta,cyan,yellow \"npm run server\" \"npm start\" \"npm run gateway\"",
        "tunnel": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/run-tunnel.ps1",
        "deploy": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/deploy.ps1"
    },
    "proxy": "http://localhost:5000",
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^9.0.1",
        "http-proxy": "^1.18.1",
        "install": "^0.13.0",
        "pkg": "^5.8.1",
        "typescript": "^4.9.5"
    },
    "pkg": {
        "assets": [
            "build/**/*",
            "Login/**/*",
            "public/**/*",
            "auth/schema.sql",
            "node_modules/sql.js/dist/sql-wasm.wasm"
        ],
        "targets": [
            "node20-linux-x64"
        ],
        "outputPath": "dist"
    }
}
